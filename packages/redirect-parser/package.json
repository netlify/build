{
  "name": "@netlify/redirect-parser",
  "version": "15.0.3",
  "description": "Parses netlify redirects into a js object representation",
  "type": "module",
  "exports": "./lib/index.js",
  "main": "./lib/index.js",
  "types": "./lib/index.d.ts",
  "files": [
    "lib/**/*"
  ],
  "scripts": {
    "prebuild": "rm -rf lib",
    "build": "tsc",
    "test": "vitest run",
