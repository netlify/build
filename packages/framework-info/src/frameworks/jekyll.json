{
  "name": "jekyll",
  "category": "static_site_generator",
  "detect": {
    "npmDependencies": [],
    "excludedNpmDependencies": [],
    "configFiles": ["_config.yml"]
  },
  "watch": {
    "command": "bundle exec jekyll serve -w",
    "port": 4000
  },
  "build": {
    "command": "bundle exec jekyll build",
    "directory": "_site"
  },
  "env": {}
}
