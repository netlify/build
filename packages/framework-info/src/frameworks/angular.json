{
  "id": "angular",
  "name": "Angular",
  "category": "frontend_framework",
  "detect": {
    "npmDependencies": ["@angular/cli"],
    "excludedNpmDependencies": [],
    "configFiles": ["angular.json"]
  },
  "dev": {
    "command": "ng serve",
    "port": 4200,
    "pollingStrategies": [{ "name": "TCP" }, { "name": "HTTP" }]
  },
  "build": {
    "command": "ng build --prod",
    "directory": "dist/"
  },
  "logo": {
    "default": "/logos/angular/default.svg",
    "light": "/logos/angular/default.svg",
    "dark": "/logos/angular/default.svg"
  },
  "env": {},
  "plugins": [
    {
      "packageName": "@netlify/angular-runtime",
      "condition": { "minNodeVersion": "16.0.0", "minFrameworkVersion": "17.0.0-rc" }
    }
  ]
}
