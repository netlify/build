{
  "name": "stencil",
  "title": "Stencil",
  "category": "static_site_generator",
  "detect": {
    "npmDependencies": ["@stencil/core"],
    "excludedNpmDependencies": [],
    "configFiles": ["stencil.config.ts"]
  },
  "dev": {
    "command": "stencil build --dev --watch --serve",
    "port": 3333
  },
  "build": {
    "command": "stencil build",
    "directory": "www"
  },
  "env": { "BROWSER": "none", "PORT": "3000" },
  "plugins": []
}
