{
  "name": "docusaurus",
  "category": "static_site_generator",
  "detect": {
    "npmDependencies": ["docusaurus"],
    "excludedNpmDependencies": [],
    "configFiles": ["siteConfig.js"]
  },
  "dev": {
    "command": "docusaurus-start",
    "port": 3000
  },
  "build": {
    "command": "docusaurus-build",
    "directory": "build/<project-name>"
  },
  "env": { "BROWSER": "none" }
}
