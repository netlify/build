{
  "id": "docusaurus",
  "name": "Docusaurus",
  "category": "static_site_generator",
  "detect": {
    "npmDependencies": ["docusaurus"],
    "excludedNpmDependencies": [],
    "configFiles": ["siteConfig.js"]
  },
  "dev": {
    "command": "docusaurus-start",
    "port": 3000,
    "pollingStrategies": [{ "name": "TCP" }, { "name": "HTTP" }]
  },
  "build": {
    "command": "docusaurus-build",
    "directory": "build/<project-name>"
  },
  "staticAssetsDirectory": "static",
  "logo": {
    "default": "/logos/docusaurus/default.svg",
    "light": "/logos/docusaurus/default.svg",
    "dark": "/logos/docusaurus/default.svg"
  },
  "env": { "BROWSER": "none" },
  "plugins": []
}
