{
  "id": "remix",
  "name": "Remix",
  "category": "static_site_generator",
  "detect": {
    "npmDependencies": ["remix", "@remix-run/netlify"],
    "excludedNpmDependencies": [],
    "configFiles": ["remix.config.js"]
  },
  "dev": {
    "command": "remix watch"
  },
  "build": {
    "command": "remix build",
    "directory": "public"
  },
  "logo": {
    "default": "/logos/hugo/default.svg",
    "light": "/logos/hugo/light.svg",
    "dark": "/logos/hugo/dark.svg"
  },
  "env": {},
  "plugins": []
}
