{
  "id": "wmr",
  "name": "WMR",
  "category": "build_tool",
  "detect": {
    "npmDependencies": ["wmr"],
    "excludedNpmDependencies": [],
    "configFiles": []
  },
  "dev": {
    "command": "wmr",
    "port": 8080,
    "pollingStrategies": [{ "name": "TCP" }, { "name": "HTTP" }]
  },
  "build": {
    "command": "wmr build",
    "directory": "dist"
  },
  "logo": {
    "default": "/logos/wmr/default.svg",
    "light": "/logos/wmr/default.svg",
    "dark": "/logos/wmr/default.svg"
  },
  "env": {},
  "plugins": []
}
