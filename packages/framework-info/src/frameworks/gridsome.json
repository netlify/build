{
  "name": "gridsome",
  "category": "static_site_generator",
  "detect": {
    "npmDependencies": ["gridsome"],
    "excludedNpmDependencies": [],
    "configFiles": ["gridsome.config.js"]
  },
  "watch": {
    "command": "gridsome develop",
    "port": 8080
  },
  "build": {
    "command": "gridsome build",
    "directory": "dist"
  },
  "env": {}
}
