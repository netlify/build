{
  "functions": {
    "my_framework*": {
      "included_files": ["files/**"]
    }
  },
  "redirects": [
    {
      "from": "/from-config",
      "to": "/to-config",
      "status": 418,
      "force": true
    }
  ],
  "redirects!": [
    {
      "from": "/from-config-override",
      "to": "/to-config-override",
      "status": 418,
      "force": true
    }
  ],
  "images": {
    "remote_images": ["domain1.from-api.netlify", "domain2.from-api.netlify"]
  }
}