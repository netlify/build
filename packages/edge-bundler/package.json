{
  "name": "@netlify/edge-bundler",
  "version": "14.8.5",
  "description": "Intelligently prepare Netlify Edge Functions for deployment",
  "type": "module",
  "main": "./dist/node/index.js",
  "exports": "./dist/node/index.js",
  "files": [
    "deno/**",
    "!deno/**/*.test.ts",
    "dist/**/*.js",
    "!dist/**/*.test.js",
    "dist/**/*.d.ts",
    "shared/**"
  ],
  "scripts": {
    "build": "tsc",
    "build:dev": "tsc -w",
    "prepack": "npm run build",
    "test": "run-s build test:dev",
    "test:dev": "run-s test:dev:*",
    "test:ci": "run-s test:integration test:ci:*",
    "test:dev:vitest": "vitest run",
    "test:dev:vitest:watch": "vitest watch",
    "test:dev:deno": "deno test --allow-all deno",
    "test:ci:vitest": "vitest run --coverage",
    "test:ci:deno": "deno test --allow-all deno",
    "test:integration": "node --experimental-modules test/integration/test.js"
  },
  "keywords": [],
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/netlify/build.git",
    "directory": "packages/edge-bundler"
  },
  "bugs": {
    "url": "https://github.com/netlify/build/issues"
  },
  "author": "Netlify Inc.",
  "directories": {
    "test": "test/node"
  },
  "devDependencies": {
    "@netlify/edge-functions-bootstrap": "^2.17.1",
    "@types/node": "^18.19.111",
    "@types/semver": "^7.3.9",
    "@types/uuid": "^10.0.0",
    "@vitest/coverage-v8": "^3.0.0",
    "archiver": "^7.0.0",
    "chalk": "^5.4.0",
    "cpy": "^11.1.0",
    "nock": "^14.0.0",
    "npm-run-all2": "^6.0.0",
    "typescript": "^5.0.0",
    "vitest": "^3.0.0"
  },
  "engines": {
    "node": ">=18.14.0"
  },
  "dependencies": {
    "@import-maps/resolve": "^2.0.0",
    "ajv": "^8.11.2",
    "ajv-errors": "^3.0.0",
    "better-ajv-errors": "^1.2.0",
    "common-path-prefix": "^3.0.0",
    "env-paths": "^3.0.0",
    "esbuild": "0.25.11",
    "execa": "^8.0.0",
    "find-up": "^7.0.0",
    "get-port": "^7.0.0",
    "node-stream-zip": "^1.15.0",
    "p-retry": "^6.0.0",
    "p-wait-for": "^5.0.0",
    "parse-imports": "^2.2.1",
    "path-key": "^4.0.0",
    "semver": "^7.3.8",
    "tar": "^7.4.3",
    "tmp-promise": "^3.0.3",
    "urlpattern-polyfill": "8.0.2",
    "uuid": "^11.0.0"
  }
}
